{
  "Monitoring": {
    "FileMasks": "*.log"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning",
        "LogMonitor": "Debug"
      }
    }
  },
  "Telegram": {
    "IsEnabled": true
  },
  "AllowedHosts": "*"
}