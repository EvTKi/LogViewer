{
  "Monitoring": {
    "FileMasks": "*.log",
    "LogDirectory": "..\\..\\..\\autotest\\LogGenerator\\log",
    "ErrorPatterns": ["ERR", "ERROR", "CRITICAL"]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning"
      }
    }
  },
  "Telegram": {
    "IsEnabled": true
  },
  "Email": {
    "IsEnabled": true,
    "From": "you@gmail.com",
    "ToEmails": ["admin@example.com", "dev@example.com"]
  },
  "AllowedHosts": "*"
}